package day1213;

public class Ex6While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=65;
		while(a<=90) {
		//	System.out.printf("%c",a++);
			//%c 한글자 출력
			System.out.println((char)a++);
		}
		
		System.out.println();
		char b='a';
		//do-while문을 입력해서 a~z까지 출력하시오
		do {
			System.out.printf("%2c",b++);
		} while(b<=97);

	}

}
