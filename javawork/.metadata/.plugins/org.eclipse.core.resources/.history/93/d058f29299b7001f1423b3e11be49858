package day1211;

public class Ex1_1DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// (byte) 강제형변환
		byte a = 126;
		byte b = (byte)130;
		byte c = (byte)300;
		
		System.out.println("a = "+a);
		System.out.println("b = "+b);
		System.out.println("c = "+c);
		
		

	}

}
