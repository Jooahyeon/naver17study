package day1213;

import java.util.Scanner;

public class Ex12For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 문제 1
		 * 숫자 n을 입력하면 1부터 n까지의 합계를 구하시오.
		 * (for문을 사용할 것)
		 * 
		 */
		
		Scanner sc=new Scanner(System.in);
		
		for(int n=1;n>=1;n++) {
			System.out.print(n+"  ");
			if(n==6)
				break;
		}
		

	}

}
