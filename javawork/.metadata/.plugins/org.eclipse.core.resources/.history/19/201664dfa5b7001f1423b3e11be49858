package day1211;

public class Ex5_1DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch1='A';
		char ch2='가';
		
		System.out.println("char1 = "+ch1);
		System.out.println("char2 = "+ch2);
		
		//printf로 출력시 변환기호 1글자: %c, 문자열:%s
		
		System.out.printf("char1%c\n",ch1);
		System.out.printf("char2%c\n",ch2);
		
		

	}

}
