package day1212;

public class Ex5Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//조건(삼항)연산자
		//조건식1? 참일때 값 : 거짓일때 값
		//조건이 여러개인경우
		//조건식 1?이 참일때 값:조건식2?참일때 값:조건식3?참이때 값:거짓일때값
		//마지막은 거짓일때 값이 들어가야해
		int x,y,max;
		x=5;
		y=3;
		
		max=x>y?x;y;
		System.out.println("max="+max);
		
		
		

	}

}
