package day1211;

public class Ex4DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int year=2024;
		int month=12;
		int day=11;
		
		int year=2024;
		int month=5;
		int day=3;
		
		//2024-12-11 이런 형식으로 출력해보자
		System.out.println(year+"-"+month+"-"+day);
		System.out.println();

	}

}
